     <link href="button_styles.css" rel='stylesheet'>

  </head>

  <body id="page-top">
    <header class="masthead bg-primary text-black">
      <div class="container">
        <form name="registerform" method="POST">
          <div class="fieldlabel"><label for="userName">이름</label></div>
          <div class="formfield"><input type="text" id="userName" name="userName" maxlength="20" value="" required></div>
          <div id="name_check"></div>
          <br>
          <div class="fieldlabel"><label for="id">아이디</label></div>
          <div class="formfield"><input type="text" id="id" name="id" maxlength="20" value="" required></div>
          <div id="nickname_check"></div>
          <br>
          <br>
          <div class="fieldlabel"><label for="age">나이</label></div>
          <div class="formfield"><input type="text" id="age" name="age" maxlength="20" value="" required></div>
          <div id="nickname_check"></div>
          <br>
          <br>
          <div class="fieldlabel"><label for="phone">핸드폰번호</label></div>
          <div class="formfield"><input type="text" id="phone" name="phone" maxlength="20" value="" required></div>
          <div id="nickname_check"></div>
          <br>
          <br>
          <div class="fieldlabel"><label for="ssn">주민등록번호</label></div>
          <div class="formfield"><input type="text" id="ssn" name="ssn" maxlength="20" value="" required></div>
          <div id="nickname_check"></div>
          <br>
          <div class="fieldlabel"><label for="email01">이메일</label></div>
          <div class="formfield"><input type="text" id="email01" name="email01" size="20" maxlength="20"
               value="" autocomplete="off" required><span>@</span>
            <input id="email02" name="email02" list="domains" placeholder="도메인 입력/선택" required>
            <datalist id="domains">
              <option value="naver.com">
              <option value="daum.net">
              <option value="yahoo.co.kr">
              <option value="outlook.com">
            </datalist>
          </div>
          <div id="email01_check"></div>
          <div id="email02_check"></div>
          <br>
        
          <div class="fieldlabel"><label for="sex">성별</label></div>
          <div class="formfield"><input type="text" id="sex" name="sex" maxlength="20" value="" required></div>
          <div id="region_check"></div>
          <br>
            
            
          <div class="fieldlabel"><label for="userPw">패스워드</label></div>
          <div class="formfield">
            <input type="password" id="userPw" name="userPw" maxlength="20" autocomplete="off" required>
          </div>
          <div id="pw_check"></div>
          <br>
          <div class="btnfield">
            <input type="submit" id="reg_submit" value="회원가입">
          </div>
        </form>
      </div>
    </header>

    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg bg-secondary fixed-top text-uppercase" id="mainNav">
